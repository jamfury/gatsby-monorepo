{
  "name": "@jamfury/gatsby-plugin-i18next",
  "description": "Gatsby plugin that provides i18n support",
  "version": "0.1.2",
  "repository": "JAMfury/gatsby-monorepo",
  "keywords": [
    "react",
    "gatsby",
    "gatsby-plugin",
    "gatsby-component",
    "i18next",
    "i18n"
  ],
  "license": "MIT",
  "publishConfig": {
