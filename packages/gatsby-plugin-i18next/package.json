    "supports-color": "6.1.0"
  },
  "peerDependencies": {
    "gatsby": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.1.4",
    "gatsby": "^3.8.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "gitHead": "8009014da2276b4a8a183a1f3f6ebd4c81f08d19"
